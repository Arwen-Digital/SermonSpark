{
    "build": {
        "development": {
            "android": {
                "buildType": "apk"
            },
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_EXPRESS_API_URL": "https://dev-api.youpreacher.com"
            }
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_EXPRESS_API_URL": "https://api-stg.youpreacher.com"
            }
        },
        "production": {
            "android": {
                "buildType": "app-bundle"
            },
            "env": {
                "EXPO_PUBLIC_EXPRESS_API_URL": "https://api.youpreacher.com"
            },
            "ios": {
                "simulator": false
            }
        }
    },
    "cli": {
        "appVersionSource": "remote",
        "version": ">= 7.0.0"
    },
    "submit": {
        "production": {}
    }
}