{
    "cli": { "version": ">= 7.0.0", "appVersionSource": "remote" },
    "build": {
        "development": {
          "developmentClient": true,
          "distribution": "internal",
          "android": { "buildType": "apk" },
          "env": {
            "EXPO_PUBLIC_EXPRESS_API_URL": "https://dev-api.youpreacher.com"
          }
        },
        "preview": {
          "distribution": "internal",
          "android": { "buildType": "apk" },
          "env": {
            "EXPO_PUBLIC_EXPRESS_API_URL": "https://staging-api.youpreacher.com"
          }
        },
        "production": {
          "android": { "buildType": "app-bundle" },
          "ios": { "simulator": false },
          "env": {
            "EXPO_PUBLIC_EXPRESS_API_URL": "https://api.youpreacher.com"
          }
        }
    },
    "submit": {
        "production": {}
    }
    }
